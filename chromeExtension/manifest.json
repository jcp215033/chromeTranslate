{
    "manifest_version": 3,
    "name": "Text Extractor and Translator",
    "version": "1.0",
    "permissions": ["activeTab", "storage", "scripting", "http://127.0.0.1:5000"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}